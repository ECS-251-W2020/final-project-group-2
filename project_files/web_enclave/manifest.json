{
  "name": "Tag Parser",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Analyzes a web page's script and form tags, then writes them to a file",
  "background" : {
    "scripts" : ["background.js", "foo.js"]
  },
  "icons": {
           "128": "symbol.png" },
  "browser_action": {},
  "app": {
    "launch": {
      "local_path": "background.html"
    }
  },
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "<all_urls>",
    "tabs"
  ]
}
