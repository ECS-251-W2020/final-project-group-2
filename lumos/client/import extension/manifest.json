{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Lumos Client (temp)",
	"version": "1.0.0",
	"author": "DA",
	"description": "Passes URL to Nodejs",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": {
			"48": "icons/cookie-48-filled.png"
		},
		"default_title": "Lumos Client"
	},
	"permissions": [
		"cookies",
		"<all_urls>",
		"activeTab",
		"tabs",
		"storage"
	],
	"icons": {
		"48": "icons/cookie-48-filled.png"
	}
}
