{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Lumos Client",
	"version": "1.0.0",
	"author": "DA",
	"description": "Passes URL to Nodejs and Imports Cookies",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"128": "icons/symbol.png"
		},
		"default_title": "Lumos Client"
	},
	"permissions": [
		"cookies",
		"<all_urls>",
		"activeTab",
		"tabs",
		"storage"
	],
	"icons": {
		"48": "icons/symbol-48.png"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	  }
}
